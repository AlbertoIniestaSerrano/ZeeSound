    <widget label="Depth" type="knob">
      <pos>480,32</pos>
      <span>2,2</span>
      <values>1,5,100</values>
      <style colour="#000000" orientation=""/>
      <csound_instrument channel="depth"/>
    </widget>
    <widget label="Width" type="knob">
      <pos>560,32</pos>
      <span>2,2</span>
      <values>1,10,100</values>
      <style colour="#000000" orientation=""/>
      <csound_instrument channel="width"/>
    </widget>
    <widget label="Level" type="knob">
      <pos>640,32</pos>
      <span>2,2</span>
      <values>1,85,100</values>
      <style colour="#000000" orientation=""/>
      <csound_instrument channel="level"/>
    </widget>
  </element-list>
</ZeeInstrument>
