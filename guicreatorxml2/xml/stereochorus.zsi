    <graphic fontcolor="#80FF00" fontfamily="teletype" fontsize="8" fontstyle="normal" fontweight="bold" set="roland" texture="img/stereochorus.png">
      <scale>32</scale>
    </graphic>
    <csound-file>csd/stereochorus.csd</csound-file>
  </instrument-properties>
  <element-list>
    <widget label="Rate" type="knob">
      <pos>350,32</pos>
      <span>2,2</span>
      <values>1,10,7000</values>
